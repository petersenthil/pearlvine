<?xml version="1.0" encoding="UTF-8"?>
<layout xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto" xmlns:tools="http://schemas.android.com/tools">
    <data>
        <variable name="splashVM" type="com.pearlvinechennai.app.modules.splash.data.viewmodel.SplashVM"/>
    </data>
    <FrameLayout android:id="@+id/frame"
        android:layout_height="match_parent"
        android:layout_width="match_parent" style="@style/groupStylelight_blue_A200">
        <ImageView android:id="@+id/image"
            android:layout_gravity="center"
            android:layout_height="@dimen/_72px"
            android:layout_margin="@dimen/_100px"
            android:layout_width="@dimen/_72px"
            android:scaleType="centerCrop" imageRes="@{@drawable/img_icon_1}"/>
    </FrameLayout>
</layout>
